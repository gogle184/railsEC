= simple_form_for [:users, diary], html: { multipart: true } do |f|
  = f.input :title, label: 'タイトル', input_html: { class: 'form-control' }
  = f.input :content, label: '内容', as: :text, input_html: { rows: 10, class: 'form-control' }
  = f.input :display, label: '表示する', as: :boolean
  = f.submit class: 'btn btn-primary'
